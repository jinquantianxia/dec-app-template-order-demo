{
    "name": "dec-app-demo",
    "version": "1.0.0",
    "description": "This is a demo application",
    "main": "index.js",
    "scripts": {
        "dev": "cd src/www && npx webpack serve --hot  --mode development --env development",
        "build": "rimraf dist && npx tsc && cd src/www && npx webpack --env production",
        "mac-deploy-pre": "chmod 775 ~/Library/Application\\ Support/cyfs/services/runtime/tools/cyfs-client & chmod 775 ~/Library/Application\\ Support/cyfs/services/runtime/tools/pack-tools",
        "deploy": "npx tsc && node move_deploy.js && cd deploy && cyfs deploy",
        "sim1": "npx tsc && node deploy/src/service/entry/app_startup.js --start --simulator 1",
        "sim2": "npx tsc && node deploy/src/service/entry/app_startup.js --start --simulator 2",
        "proto-mac-pre": "npm i -g ts-protoc-gen && cd tools && chmod 775 protoc",
        "proto-mac": "cd tools && ./protoc --proto_path=../src/common/objs --js_out=import_style=commonjs,binary:../src/common/objs --ts_out=../src/common/objs/ ../src/common/objs/obj_proto.proto",
        "proto-windows": "cd tools && protoc.exe --proto_path=../src/common/objs --js_out=import_style=commonjs,binary:../src/common/objs --ts_out=../src/common/objs/ ../src/common/objs/obj_proto.proto",
        "lint": "eslint \"**/*.{ts,tsx}\"",
        "lint-fix": "eslint \"**/*.{ts,tsx}\" --fix"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@babel/core": "^7.14.3",
        "@babel/preset-env": "^7.14.2",
        "@babel/preset-react": "^7.13.13",
        "@emotion/react": "^11.7.0",
        "@emotion/styled": "^11.6.0",
        "@pmmmwh/react-refresh-webpack-plugin": "^0.5.3",
        "@types/fs-extra": "^9.0.13",
        "@types/lodash": "^4.14.171",
        "@types/react-dom": "^17.0.9",
        "@types/react-router-dom": "^5.1.8",
        "@typescript-eslint/eslint-plugin": "^4.33.0",
        "@typescript-eslint/parser": "^4.33.0",
        "babel-loader": "^8.2.2",
        "crypto-browserify": "^3.12.0",
        "css-loader": "^6.3.0",
        "esbuild": "^0.14.1",
        "esbuild-loader": "^2.15.1",
        "eslint": "^7.32.0",
        "eslint-config-alloy": "^4.4.0",
        "eslint-loader": "^4.0.2",
        "eslint-plugin-react": "^7.28.0",
        "eslint-plugin-react-hooks": "^4.3.0",
        "file-loader": "^6.2.0",
        "fs-extra": "^10.0.0",
        "html-webpack-plugin": "^5.3.2",
        "less": "^4.1.2",
        "less-loader": "^10.2.0",
        "node-typescript-compiler": "^2.2.1",
        "prettier": "^2.5.1",
        "react-refresh": "^0.11.0",
        "stream-browserify": "^3.0.0",
        "style-loader": "^3.3.0",
        "style-resources-loader": "^1.5.0",
        "ts-protoc-gen": "^0.15.0",
        "typescript": "^4.5.2",
        "url-loader": "^4.1.1",
        "webpack": "^5.64.4",
        "webpack-cli": "^4.9.1",
        "webpack-dev-server": "^4.6.0"
    },
    "dependencies": {
        "antd": "^4.18.3",
        "cyfs-sdk": "npm:cyfs-sdk-nightly@^0.5.9",
        "google-protobuf": "^3.20.1",
        "path": "^0.12.7",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-router-dom": "^6.0.2"
    }
}
